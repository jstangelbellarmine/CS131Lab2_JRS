/*
 * Lockable Interface
 * +setKey(int key): void
 * +lock(int key): void
 * +unlock(int key): void
 * +isLocked(): boolean
 */

public interface Lockable {

	public abstract int setKey(int key);
	public  abstract int lock( int key);
	public  abstract int unlock(int key);
	public abstract boolean isLocked();

}//end interface
